record (ao, "k220:setI")
{
	field (DESC, "set I of K220")
	field (DTYP, "stream")
    field (OUT, "@k220.proto setI $(PORT) $(G)")
    field (PINI, "0")
    field (TPRO, "1")
	field (SCAN, "Passive")
	field (OMSL, "0")
}

